{
  "name": "zmey",
  "version": "1.0.0",
  "description": "workflows manager",
  "main": "bundle.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config ./client/webpack.prod.config.js",
    "wds": "./node_modules/.bin/webpack-dev-server --config ./client/webpack.dev.config.js",
    "server": "export FLASK_APP=./server&& export FLASK_ENV=development&& flask run",
    "serverWin": "set FLASK_APP=./server&& set FLASK_ENV=development&& flask run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tyill/zmey.git"
  },
  "author": "a.medved",
  "bugs": {
    "url": "https://github.com/Tyill/zmey/issues"
  },
  "homepage": "https://github.com/Tyill/zmey#readme",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-typescript",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      [
        "@babel/plugin-transform-typescript",
        {
          "allowNamespaces": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.5.0",
    "css-loader": "^3.2.0",
    "file-loader": "^6.2.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.3",
    "react-hot-loader": "^4.12.12",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "flask": "^0.2.10"
  }
}
